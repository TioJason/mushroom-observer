<div class="list-group-item">
  <div class="row">
    <small>
    <div class="col-sm-2">
    </div>
    <div class="col-sm-4">
      <%= link_with_query(vote.naming.format_name.t,
          controller: :name,
          action: :show_name,
          id: vote.naming.name) %>
    </div>
    <div class="col-sm-2">
      <%= vote.created_at.strftime("%Y-%m-%d") %>
    </div>
    <div class="col-sm-4">
      <%= user_link(vote.user, vote.user.login) %>:
      <%= vote.value_name %>
    </div>
    </small>
  </div>
</div>
